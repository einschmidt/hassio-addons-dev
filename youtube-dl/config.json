{
  "name": "youtube-dl dev",
  "version": "2021-01-19-17-26",
  "slug": "youtube-dl-dev",
  "description": "Download videos from youtube.com and a few more sites",
  "url": "https://github.com/berichta/hassio-addons",
  "arch": [
    "armhf",
    "armv7",
    "amd64",
    "aarch64",
    "i386"
  ],
  "startup": "once",
  "boot": "manual",
  "map": [
    "share:rw"
  ],
  "options": {
    "global_options": [
      "-o /share/youtube-dl/%(title)s.%(ext)s"
    ],
    "options": [],
    "URL": [
      "https://test-streams.mux.dev/x36xhzz/x36xhzz.m3u8"
    ],
    "log_level": "info"
  },
  "schema": {
    "global_options": [
      "str"
    ],
    "options": [
      "str"
    ],
    "URL": [
      "str"
    ],
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)"
  },
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  },
  "hassio_role": "default",
  "hassio_api": true,
  "image": "berichta/youtube-dl-{arch}"
}
