{
    "name": "Radicale",
    "version": "0.0.1",
    "slug": "radicale",
    "description": "CalDAV and CardDAV Server",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "application",
    "boot": "auto",
    "map": ["share"],
    "ports": {
        "5232/tcp": 5232
    },
    "options": {
        "log_level": "info",
        "args": []
    },
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
        "args": ["str"]
    },
    "image": "berichta/radicale-{arch}"
  }