{
    "name": "TT-RSS",
    "version": "test",
    "slug": "ttrss",
    "description": "RSS Aggregator",
    "url": "https://github.com/einschmidt/hassio-addons/tree/master/ttrss",
    "arch": ["armhf", "armv7", "amd64", "aarch64"],
    "startup": "services",
    "boot": "auto",
    "hassio_api": true,
    "hassio_role": "default",
    "map": ["share", "config", "ssl:rw"],
    "host_network": true,
    "options": {
        "log_level": "info"
    },
    "schema": {
        "log_level": "list(trace|debug|info|notice|warning|error|fatal)"
    },
    "environment": {
        "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
    },
    "image": "berichta/ttrss-{arch}"
}
